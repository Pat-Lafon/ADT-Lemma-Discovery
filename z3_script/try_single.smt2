(declare-fun p (Int Int) Bool)
(declare-const x Int)
(declare-const y Int)
(declare-const u Int)
(assert (= (p x u) false))
(assert (= (p y u) false))

(assert
 (and (forall ((w Int) (v Int))
              (and (= (p x w) (p x v))
                   (= (p y w) (p y v))
                   )
              )
      (and (=> (forall ((v Int)) (p x v))
              (forall ((w Int)) (p y w))
              )
           (not false)
           )
      )
 )
(check-sat)
(get-model)
